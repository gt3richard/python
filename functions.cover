       # bytes
       # Same as bytearray but immutable (not changeable)
    1: assertEqual(bytes(5), bytes(b'\x00\x00\x00\x00\x00')) #Initialized an array of 5 empty bytes
    1: assertEqual(bytes([5]), bytes(b'\x05'))
    1: assertEqual(bytes('Test', 'UTF-8'), bytes(b'Test'))
